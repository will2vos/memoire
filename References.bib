@article{Pollierer2021Diversityfunctional,
	title = {Diversity and functional structure of soil animal communities suggest soil animal food webs to be buffered against changes in forest land use},
	volume = {196},
	issn = {1432-1939},
	url = {https://doi.org/10.1007/s00442-021-04910-1},
	doi = {10.1007/s00442-021-04910-1},
	abstract = {Forest soil and litter is inhabited by a diverse community of animals, which directly and indirectly rely on dead organic matter as habitat and food resource. However, community composition may be driven by biotic or abiotic forces, and these vary with changes in habitat structure and resource supply associated with forest land use. To evaluate these changes, we compiled comprehensive data on the species composition of soil animal communities and environmental factors in forest types varying in land-use intensity in each of three regions in Germany, i.e., coniferous, young managed, old managed, and unmanaged beech forests. Coniferous forests featured high amounts of leaf litter and low microbial biomass concentrations contrasting in particular unmanaged beech forests. However, soil animal diversity and functional community composition differed little between forest types, indicating resilience against disturbance and forest land use. Structural equation modelling suggested that despite a significant influence of forest management on resource abundance and quality, the biomass of most soil fauna functional groups was not directly affected by forest management or resource abundance/quality, potentially because microorganisms hamper the propagation of nutrients to higher trophic levels. Instead, detritivore biomass depended heavily on soil pH. Macrofauna decomposers thrived at high pH, whereas mesofauna decomposers benefitted from low soil pH, but also from low biomass of macrofauna decomposers, potentially due to habitat modification by macrofauna decomposers. The strong influence of soil pH shows that decomposer communities are structured predominantly by regional abiotic factors exceeding the role of local biotic factors such as forest type.},
	language = {en},
	number = {1},
	urldate = {2024-04-14},
	journal = {Oecologia},
	author = {Pollierer, Melanie M. and Klarner, Bernhard and Ott, David and Digel, Christoph and Ehnes, Roswitha B. and Eitzinger, Bernhard and Erdmann, Georgia and Brose, Ulrich and Maraun, Mark and Scheu, Stefan},
	month = may,
	year = {2021},
	keywords = {Disturbance, Forest management, Invertebrates, Soil pH, Structural equation modelling},
	pages = {195--209},
	file = {Full Text PDF:/Users/williamdevos/Zotero/storage/USDFGLA3/Pollierer et al. - 2021 - Diversity and functional structure of soil animal .pdf:application/pdf},
}

@article{Walton2005Contrastingeffects,
	title = {Contrasting effects of salamanders on forest-floor macro- and mesofauna in laboratory microcosms},
	volume = {49},
	issn = {00314056},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0031405604000976},
	doi = {10.1016/j.pedobi.2004.07.012},
	abstract = {We employed laboratory microcosms to test the hypothesis that predation by the salamander, Plethodon cinereus, alters invertebrate species composition of a terrestrial detritus-based food web and thereby inﬂuences leaf–litter decomposition rates. We comparedh litter invertebrates among samples of leaf–litter substrate samples in microcosms either with or without individual salamanders. We used mesh leaf–litter bags to quantify leaf–litter decomposition and temporal changes in invertebrate abundance and community composition during colonization of new litter by invertebrates. We report that substrates within microcosms with P. cinereus had more podomorphic Collembola, fewer enchytraeid worms and fewer macroinvertebrate detritivores than controls at the end of the 3 month experiment. Total invertebrate density, and densities of podomorphic Collembola and Acari increased more rapidly in litter bags within salamander microcosms than in controls. In contrast, entomobryid Collembola were fewer in litter bags in salamander microcosms in comparison to controls. However, salamanders had no effect on leaf–litter decomposition rates. We hypothesize that salamander predation enhanced microbidetritivore mesofauna densities by (1) reducing numbers of larger detritivores that compete with mesofauna, and/or by (2) providing a subsidy in the form of salamander feces that promoted growth of microﬂora. Further, we hypothesize that increases of mesofauna in salamander microcosms compensated for effects on litter decomposition that may have resulted from reductions in detritivorous macrofauna.},
	language = {en},
	number = {1},
	urldate = {2023-08-02},
	journal = {Pedobiologia},
	author = {Walton, B. Michael and Steckler, Sonya},
	month = jan,
	year = {2005},
	keywords = {brook experimental forest, decomposition, red-backed salamander, amphibians, terrestrial salamander, litter decomposition, soil, dynamics, detrital food web, detrital food webs, indirect effects, new-hampshire, plethodontid salamanders, plethodontidae, spider predation, top-down control, notion},
	pages = {51--60},
	file = {Walton_Steckler_2005_Contrasting effects of salamanders on forest-floor macro- and mesofauna in.pdf:/Users/williamdevos/Documents/Biblio_zotfile/Salamandres/Walton_Steckler_2005_Contrasting effects of salamanders on forest-floor macro- and mesofauna in.pdf:application/pdf},
}

