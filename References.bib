	pages = {791--795},
	file = {Warren et al_2018_The projected effect on insects, vertebrates, and plants of limiting global.pdf:/Users/williamdevos/Documents/Biblio_zotfile/Climate change/Warren et al_2018_The projected effect on insects, vertebrates, and plants of limiting global.pdf:application/pdf},
}

