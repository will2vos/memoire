@article{McCarthy1994Distributionabundance,
	title = {Distribution and abundance of coarse woody debris in a managed forest landscape of the central {Appalachians}},
	volume = {24},
	issn = {0045-5067},
	url = {https://cdnsciencepub.com/doi/10.1139/x94-172},
	doi = {10.1139/x94-172},
	abstract = {Coarse woody debris (CWD) is integral to the functioning and productivity of forested ecosystems. Standing snags and large logs on the forest floor affect soil processes, soil fertility, hydrology, and wildlife microhabitat. Few data are available pertaining to the distribution and abundance of CWD in the managed hardwood forests of the central Appalachians. We surveyed 11 stands, at various stages of development (succession) after clear-cutting ({\textless}2, 15–25, 65–90, {\textgreater}100 years old), to evaluate the density, volume, and biomass of trees, snags, and logs under the local forest management regime. As expected, density, volume, and biomass of CWD (stems ≥2.5 cm diameter) were greatest in young stands ({\textless}2 years old) immediately following clear-cutting; the vast majority of CWD existed as relatively labile, small-diameter, low decay state logging slash. Young stands retained a few large logs in advanced decay states but observations suggest that these elements were often disturbed (i.e., crushed) by logging equipment during the harvest process. Crushed logs do not function ecologically in the same capacity as large intact logs. A marked decline in CWD was observed in young pole stands (15–25 years old) as slash decomposed. These stands were characterized by a high density of young hardwood stump sprouts in the overstory while maintaining a moderate amount of CWD in middle size and decay states on the forest floor. More mature hardwood stands (65–90 years old) generally exhibited a decrease in live-stem density and an increase in basal area, accompanied by a slight increase in CWD. Commercial thinning presumably limits the contribution of large CWD to the forest floor. This was most clearly evident in the oldest stands ({\textgreater}100 years old) where large CWD was not widely observed. A striking feature across all stands was the near absence of logs in large size classes ({\textgreater}65 cm diameter) and a paucity of logs in mid to late decay stages. We discuss our data in the context of hardwood forest structure and management in the central Appalachians.},
	number = {7},
	urldate = {2024-10-28},
	journal = {Canadian Journal of Forest Research},
	author = {McCarthy, Brian C. and Bailey, Ronald R.},
	month = jul,
	year = {1994},
	pages = {1317--1329},
	file = {Full Text PDF:/Users/williamdevos/Zotero/storage/6A4RZJGM/McCarthy et Bailey - 1994 - Distribution and abundance of coarse woody debris in a managed forest landscape of the central Appal.pdf:application/pdf},
}

@article{Chen1999Microclimateforest,
	title = {Microclimate in forest ecosystem and landscape ecology: variations in local climate can be used to monitor and compare the effects of different management regimes},
	volume = {49},
	issn = {0006-3568},
	shorttitle = {Microclimate in forest ecosystem and landscape ecology},
